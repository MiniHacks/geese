{
  "name": "geese manifest title!",
  "description": "template made by @minihacks/geese",
  "version": "1.2.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "omnibox": {
    "keyword": "gg"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "css": [
        "injection/injection.css"
      ],
      "js": [
        "injection/injection.js"
      ]
    }
  ],
  "action": {
    "default_title": "geese action title!",
    "default_icon": {
      "32": "icons/icon-32.png",
      "72": "icons/icon-72.png",
      "128": "icons/icon-128.png",
      "512": "icons/icon-512.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "32": "icons/icon-32.png",
    "72": "icons/icon-72.png",
    "128": "icons/icon-128.png",
    "512": "icons/icon-512.png"
  },
  "permissions": [
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ]
}
